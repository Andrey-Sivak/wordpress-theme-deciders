  "scripts": {
    "guten-build": "wp-scripts build --webpack-src-dir=guten/src/blocks --output-path=guten/build",
    "guten-format": "wp-scripts format --webpack-src-dir=guten/src/blocks --output-path=guten/build",
    "guten-lint:css": "wp-scripts lint-style --webpack-src-dir=guten/src/blocks --output-path=guten/build",
    "guten-lint:js": "wp-scripts lint-js --webpack-src-dir=guten/src/blocks --output-path=guten/build",
    "guten-packages-update": "wp-scripts packages-update",
    "guten-plugin-zip": "wp-scripts plugin-zip",
    "guten-start": "wp-scripts start --webpack-src-dir=guten/src/blocks --output-path=guten/build",
    "dev": "gulp",
    "build": "gulp build --build",
    "svgSprive": "gulp svgSprive",
    "prezip": "npm run guten-build && npm run build",
    "zip": "dir-archiver --src . --dest ./op.zip --exclude .git .vs node_modules gulp assets guten/src .idea .github .husky gulp guten/node_modules tailwind-settings .commitlintrc.json .gitignore .prettierignore .prettierrc .prettierrc.json .stylelintrc.json .versionrc.json gulpfile.js LICENSE package.json package-lock.json phpcs.xml README.md readme.txt tailwind.config.cjs composer.lock composer.json",
    "lint": "eslint ./src/js --cache && echo \"No linting errors\"",
    "lint:fix": "eslint ./src/js --fix",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  }
}
